{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Swal from \"sweetalert2\";\nexport default {\n  data() {\n    return {\n      Email: \"\",\n      UserPassword: \"\",\n      isLoading: false\n    };\n  },\n  computed: {\n    message() {\n      return this.$store.state.message;\n    }\n  },\n  methods: {\n    async login() {\n      this.isLoading = true;\n      const userData = {\n        Email: this.Email,\n        UserPassword: this.UserPassword\n      };\n      try {\n        await this.$store.dispatch(\"login\", userData);\n        Swal.fire(\"Success\", \"Login successful\");\n        // Redirect the user to the program page on successful login\n        this.$router.push(\"/programs\");\n      } catch (error) {\n        console.log(error.message);\n      } finally {\n        this.isLoading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["Swal","data","Email","UserPassword","isLoading","computed","message","$store","state","methods","login","userData","dispatch","fire","$router","push","error","console","log"],"sources":["C:\\Users\\Thabang\\BookingAir\\ui\\src\\components\\Login.vue"],"sourcesContent":["<template>\r\n  <body>\r\n    <h1 class=\"text-center animate__animated animate__zoomIn\">Login</h1>\r\n    <div class=\"container\">\r\n      <div class=\"row\">\r\n        <div class=\"col-md-6 image1\"></div>\r\n        <div class=\"col-md-6 right\">\r\n          <div class=\"inputText\">\r\n            <header class=\"text-black pb-5 fw-bold\">Login</header>\r\n            <form @submit.prevent=\"login\">\r\n              <div>\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"input\"\r\n                  v-model=\"Email\"\r\n                  placeholder=\"Email\"\r\n                  required\r\n                />\r\n              </div>\r\n              <div>\r\n                <input\r\n                  type=\"password\"\r\n                  class=\"input\"\r\n                  v-model=\"UserPassword\"\r\n                  placeholder=\"Password\"\r\n                  required\r\n                />\r\n              </div>\r\n              <div>\r\n                <button class=\"submit\" :disabled=\"isLoading\">\r\n                  <span v-if=\"isLoading\">\r\n                    <i class=\"fa-solid fa-circle-notch fa-spin\"></i> Loading...\r\n                  </span>\r\n                  <span v-else> Submit </span>\r\n                </button>\r\n              </div>\r\n            </form>\r\n            <div class=\"login fw-bold text-black\">\r\n              <span\r\n                >Don't have an account?\r\n                <a href=\"#\"\r\n                  ><router-link to=\"/registration\">Register</router-link>\r\n                </a></span\r\n              >\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </body>\r\n</template>\r\n<script>\r\nimport Swal from \"sweetalert2\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      Email: \"\",\r\n      UserPassword: \"\",\r\n      isLoading: false,\r\n    };\r\n  },\r\n  computed: {\r\n    message() {\r\n      return this.$store.state.message;\r\n    },\r\n  },\r\n  methods: {\r\n    async login() {\r\n      this.isLoading = true;\r\n      const userData = {\r\n        Email: this.Email,\r\n        UserPassword: this.UserPassword,\r\n      };\r\n      try {\r\n        await this.$store.dispatch(\"login\", userData);\r\n        Swal.fire(\"Success\", \"Login successful\");\r\n        // Redirect the user to the program page on successful login\r\n        this.$router.push(\"/programs\");\r\n      } catch (error) {\r\n        console.log(error.message);\r\n      } finally {\r\n        this.isLoading = false;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nbody {\r\n  background: radial-gradient(\r\n    circle,\r\n    rgba(248, 248, 248, 1) 0%,\r\n    rgb(193, 210, 232) 100%\r\n  );\r\n  font-family: